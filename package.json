    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "graphql": "^16.8.2",
    "mocha": "^9.2.1",
    "offline-github-changelog": "^3.1.0",
    "prettier": "2.5.1",
    "unexpected": "^12.0.4",
